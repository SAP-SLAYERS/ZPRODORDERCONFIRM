<core:FragmentDefinition
    xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core"
>
    <ValueHelpDialog
        id="operationVHdialog"
        title="Activity"
        ok=".onOPOkPress"
        cancel=".onOPCancelPress"
        afterClose=".onOPAfterClose"
        supportRanges="false"
        key="Operation"
        descriptionKey="Activity"
    >
        <filterBar>
            <fb:FilterBar
                id="operationVHFilterBar"
                advancedMode="true"
                search=".onOPSearch"
                isRunningInValueHelpDialog="true"
            >
                <fb:filterGroupItems>
                    <fb:FilterGroupItem
                        id="oerationVH_ActivityItem"
                        groupName="__$INTERNAL$"
                        name="Activity"
                        label="Activity"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:Input id="_IDGenInput30"
                                name="Activity"
                            />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        id="operationVH_SequenceItem"
                        groupName="__$INTERNAL$"
                        name="Sequence"
                        label="Sequence"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:Input
                                id="seqInput"
                                name="Sequence"
                            />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        id="operationVH_OperationQtyItem"
                        groupName="__$INTERNAL$"
                        name="Operation Quantity"
                        label="Operation Quantity"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:Input
                                id="OPqtyInput"
                                name="Operation Quantity"
                            />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        id="orderVH_OperationUnit"
                        groupName="__$INTERNAL$"
                        name="Operation Unit"
                        label="Operation Unit"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:Input
                                id="OpUnitInput"
                                name="Operation Unit"
                            />
                        </fb:control>
                   
                           
                    </fb:FilterGroupItem>

                    <fb:FilterGroupItem
                        id="operationVH_OperationShorttxtItem"
                        groupName="__$INTERNAL$"
                        name="Operation Short Text"
                        label="Operation Short Text"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:Input
                                id="OPShortTextInput"
                                name="Operation Short Text"
                            />
                        </fb:control>
                    </fb:FilterGroupItem>

                    <fb:FilterGroupItem
                        id="operationVH_yieldItem"
                        groupName="__$INTERNAL$"
                        name="Confirmed Yield"
