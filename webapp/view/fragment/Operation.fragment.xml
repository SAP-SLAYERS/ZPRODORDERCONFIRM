                            <m:Input
                                id="OPScrapInput"
                                name="Confirmed Scrap"
                            />
                        </fb:control>
                    </fb:FilterGroupItem>

                    <fb:FilterGroupItem
                        id="operationVH_OperationReworkItem"
                        groupName="__$INTERNAL$"
                        name="Confirmed Rework"
                        label="Confirmed Rework"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:Input
                                id="OPReworkInput"
                                name="Confirmed Rework"
                            />
